{
  "name": "seee-expedition-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:http": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:arch": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest",
    "test:e2e": "cross-env INSTRUMENT_CODE=1 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:bdd:gen": "npx bddgen -c playwright.bdd.config.ts",
    "test:bdd": "npm run test:bdd:gen && cross-env INSTRUMENT_CODE=1 playwright test -c playwright.bdd.config.ts",
    "test:bdd:ui": "playwright test -c playwright.bdd.config.ts --ui",
    "test:bdd:coverage": "npm run test:unit && npm run test:bdd && npm run test:merge",
    "test:req-tags": "bash ./scripts/check_req_tags.sh",
    "test:mutation": "stryker run",
    "pretest:merge": "mkdir -p coverage/merged",
    "test:merge": "nyc merge coverage/unit coverage/merged/unit.json && nyc merge coverage/e2e coverage/merged/e2e.json && nyc merge coverage/merged coverage/merged/coverage.json",
    "posttest:merge": "nyc report --reporter=html --reporter=text --temp-dir=coverage/merged --report-dir=coverage/total",
    "audit:coverage": "npm run test:unit && npm run test:e2e && npm run test:merge",
    "validate:safety": "./scripts/validate-safety-layer.sh",
    "tscheck": "tsc --noEmit 2>&1 | grep -v '.next/types/validator.ts' || true",
    "validate": "npm run tscheck && npm run lint && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-table": "^8.21.3",
    "bottleneck": "^2.19.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.555.0",
    "next": "15.0.7",
    "next-auth": "^4.24.13",
    "pino": "^10.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@cucumber/cucumber": "^12.4.0",
    "@playwright/test": "^1.57.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "^15.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.12.3",
    "nyc": "^17.1.0",
    "pino-pretty": "^13.1.3",
    "playwright-bdd": "^8.4.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "undici": "^6.19.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
